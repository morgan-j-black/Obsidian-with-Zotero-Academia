@article{reimchen2008,
  title = {Assessing Niche Differences of Sex, Armour and Asymmetry Phenotypes Using Stable Isotope Analyses in {{Haida Gwaii}} Sticklebacks},
  author = {{Reimchen} and {Ingram} and {Hansen}},
  year = {2008},
