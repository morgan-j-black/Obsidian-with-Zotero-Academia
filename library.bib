@article{silver2022,
  title = {Fish, {{People}}, and {{Systems}} of {{Power}}: {{Understanding}} and {{Disrupting Feedback}} between {{Colonialism}} and {{Fisheries Science}}},
  shorttitle = {Fish, {{People}}, and {{Systems}} of {{Power}}},
  author = {Silver, Jennifer J. and Okamoto, Daniel K. and Armitage, Derek and Alexander, Steven M. and Atleo (Kam'ayaam/Chachim'multhnii), Clifford and Burt, Jenn M. and Jones (Nang Jingwas), Russ and Lee, Lynn C. and Muhl, Ella-Kari and Salomon, Anne K. and Stoll, Joshua S.},
