@article{chenery2005,
  title = {Detecting {{Anthropogenic Stress}} in an {{Ecosystem}}: 3. {{Mesoscale Variability}} and {{Biotic Indices}}},
  shorttitle = {Detecting {{Anthropogenic Stress}} in an {{Ecosystem}}},
  author = {Chenery, Anna M. and Mudge, Stephen M.},
  year = {2005},
  month = dec,
  journal = {Environmental Forensics},
  volume = {6},
  number = {4},
