@article{huang2014,
  title = {Spatial Distribution and Reconstruction Potential of {{Japanese}} Anchovy ({{Engraulis}} Japonicus) Based on Scale Deposition Records in Recent Anaerobic Sediment of the {{Yellow Sea}} and {{East China Sea}}},
  author = {Huang, Jiansheng and Sun, Yao and Jia, Haibo and Yang, Qian and Tang, Qisheng},
