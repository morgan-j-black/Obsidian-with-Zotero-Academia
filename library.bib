@article{daros2021,
  title = {Restoration of {{Cymodocea}} Nodosa Seagrass Meadows: Efficiency and Ecological Implications},
  shorttitle = {Restoration of {{{\emph{Cymodocea}}}}{\emph{ Nodosa}} Seagrass Meadows},
  author = {Da Ros, Zaira and Corinaldesi, Cinzia and Dell'Anno, Antonio and Gambi, Cristina and Torsani, Fabrizio and Danovaro, Roberto},
