@article{groesbeck2014,
  title = {Ancient {{Clam Gardens Increased Shellfish Production}}: {{Adaptive Strategies}} from the {{Past Can Inform Food Security Today}}},
  shorttitle = {Ancient {{Clam Gardens Increased Shellfish Production}}},
  author = {Groesbeck, Amy S. and Rowell, Kirsten and Lepofsky, Dana and Salomon, Anne K.},
