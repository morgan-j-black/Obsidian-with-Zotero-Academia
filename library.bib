  author = {{Vertebrate Pest Research Unit, New South Wales Department of Primary Industries, 1447 Forest Road, Orange, NSW 2800, Australia} and Forsyth, Dave and Perry, Mike and Moloney, Paul and McKay, Meredith and Gormley, Andrew and Warburton, Bruce and Sweetapple, Peter and Dewhurst, Robyn},
  year = {2018},
