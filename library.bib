  author = {Arnould, John P. Y. and Monk, Jacquomo and Ierodiaconou, Daniel and Hindell, Mark A. and Semmens, Jayson and Hoskins, Andrew J. and Costa, Daniel P. and Abernathy, Kyler and Marshall, Greg J.},
