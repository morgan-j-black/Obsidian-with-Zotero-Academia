@article{blanchet2008,
  title = {An Integrated Comparison of Captive-Bred and Wild {{Atlantic}} Salmon ({{Salmo}} Salar): {{Implications}} for Supportive Breeding Programs},
  shorttitle = {An Integrated Comparison of Captive-Bred and Wild {{Atlantic}} Salmon ({{Salmo}} Salar)},
