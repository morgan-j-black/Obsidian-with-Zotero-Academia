  shorttitle = {Size {{Structure}} of {{Marine Soft-Bottom Macrobenthic Communities}} across {{Natural Habitat Gradients}}},
  author = {Macdonald, Tara A. and Burd, Brenda J. and {van Roodselaar}, Albert},
