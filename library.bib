@article{hooper2012,
  title = {A Global Synthesis Reveals Biodiversity Loss as a Major Driver of Ecosystem Change},
  author = {Hooper, David U. and Adair, E. Carol and Cardinale, Bradley J. and Byrnes, Jarrett E. K. and Hungate, Bruce A. and Matulich, Kristin L. and Gonzalez, Andrew and Duffy, J. Emmett and Gamfeldt, Lars and O'Connor, Mary I.},
  year = {2012},
